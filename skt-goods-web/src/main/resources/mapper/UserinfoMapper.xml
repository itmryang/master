<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.skt.mapper.UserInfoMapper">


    <insert id="insertUserInfo" parameterType="com.skt.dto.UserInfo" keyProperty="id" useGeneratedKeys="true">
         insert into user_info(uid,nick_name,gender,avatar) values(#{uid}, #{nick_name},#{gender},#{avatar})
    </insert>

</mapper>